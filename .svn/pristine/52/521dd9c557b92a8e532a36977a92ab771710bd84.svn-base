	  <update id="updateByMoveNo" parameterType="com.penghaisoft.wms.storagemanagement.model.entity.WmsMoveStereoscopic">
	    UPDATE wms_move_stereoscopic
	    SET active_flag = '0'
	    ,last_modified_by = #{entity.lastModifiedBy}
	    ,gmt_modified = #{entity.gmtModified}
	     where move_no = #{entity.moveNo} and active_flag = '1'
	  </update>

	  <update id="batchDelete" parameterType="com.penghaisoft.wms.storagemanagement.model.entity.WmsMoveStereoscopic">
	    DELETE FROM wms_move_stereoscopic
	    where move_id in 
	    <foreach collection="entity.moveIdList" index="index" item="item" open="(" separator="," close=")">
	      #{item}
	    </foreach>
	  </update>

