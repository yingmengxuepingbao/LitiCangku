spring:
  banner:
    location: banner.txt
    charset: UTF-8
  application:
    name: pda
  profiles:
    active: dev

server:
  port: 9091
  servlet:
    context-path: /pda

logging:
  config: classpath:logback-spring.xml
  path: E:\logs
  level:
    root: info
    web: info

mybatis:
  mapper-locations: classpath:mapping/*/*.xml
  configuration:
    log-impl: org.apache.ibatis.logging.stdout.StdOutImpl

wcs:
  config:
    applyfactory: DEFAULT