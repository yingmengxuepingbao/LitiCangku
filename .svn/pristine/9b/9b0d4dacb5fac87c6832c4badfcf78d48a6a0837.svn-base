	  </update>

	<!-- 任意字段全匹配 -->
	<select id="queryByAny" resultMap="wcsLocationRealMap" >
		<include refid="selectAllColumns"/>
		<include refid="where_any" />
	</select>
    <select id="queryByIds" resultMap="wcsLocationRealMap">
		<include refid="selectAllColumns" />
		where wcs_location_real_.location_id in
		<foreach collection="locationIds" index="index" item="locationId" open="(" separator="," close=")">
		  #{locationId,jdbcType=INTEGER}
		</foreach>
	</select>
</mapper>