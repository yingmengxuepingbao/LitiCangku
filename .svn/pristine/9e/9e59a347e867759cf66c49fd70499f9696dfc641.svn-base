.fa-table-line-save:hover:after,
.fa-table-line-edit:hover:after,
.fa-table-line-delete:hover:after,
.fa-table-line-check:hover:after,
.fa-table-line-over:hover:after,
.fa-table-line-receive:hover:after,
.fa-table-line-undo:hover:after,
.fa-table-line-submit:hover:after,
.fa-table-line-fhCheck:hover:after{
	width: 90px;
	height: 30px;
	position: absolute;
	top:25px;
	left:-2px;
