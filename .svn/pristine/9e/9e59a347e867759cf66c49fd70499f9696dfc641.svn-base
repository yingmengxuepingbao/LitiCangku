}
.fa-table-line-edit{
	background-image: url("../images/common/table-line-edit.png");
}
.fa-table-line-delete{
	background-image: url("../images/common/table-line-delete.png");
}
.fa-table-line-check{
	background-image: url("../images/common/table-line-check.png");
}
.fa-table-line-over{
