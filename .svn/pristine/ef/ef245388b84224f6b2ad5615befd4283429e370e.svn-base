.icon-jiantou9{
	font-size:10px ;
}

.menu-ms{
	width: 70px;
}

.menu-ms{
	width: 70px;
}


.menu-ms #control_bar{
	display: none;
}
.menu-ms #menu p{
	cursor: pointer;
	user-select: none;
	display: block;
	height: 45px;
	line-height: 45px;
	margin: 0;
	font-size: 12px;
	width: 100%;
}

.menu-ms #menu>ul>li>p{
	line-height: 16px;
}
.menu-ms #menu li img{
	width: 20px;
	position: relative;
	/*top: -2px;*/
	margin: 10px;
	padding: 0;
	/*line-height: 20px;*/
}

.menu-ms #menu p:hover{
	cursor: pointer;
	color: #fff;
	background: rgba(255,255,255,.05);
}
.menu-ms #menu p:active{
	cursor: pointer;
	background: rgba(255,255,255,.1);
}
.menu-ms #menu p span{
	display: none;
}
.menu-ms #menu p .textContainer{
	display: block;
}
.menu-ms #menu p .fontspan{
	display: inline-block;
	line-height: 18px;
	height: 18px;
}
.menu-ms #menu>ul>li{
	position: relative;
}
.menu-ms #menu>ul>li>ul{
	position: absolute;
	z-index: 10000;
	width: 160px;
	left: 70px;
	top: 0;
	background:#2A3F54 ;
}
.menu-ms #menu>ul>li>p{
	height: 67px;
	text-align: center;
}
.menu-ms #menu>ul>li>p>.textContainer{
	width: 100%;
	border-left:1px solid rgba(66,86,104,0.9);
}

.menu-ms #menu>ul>li>p>.textContainer>.fontspan{
	display: block;
	display: none;
	font-size: 24px;
	text-align: center;
	line-height: 40px;
}
.menu-ms #userinfomation{
	display: none;
}

.menu-ms #menu p .menuname{
	display: inline-block;
	font-size: 20px;
	/*line-height: 50px;*/
}
.menu-ms #menu>ul>li>p .menuname{
	display: block;
	font-size: 12px;
}
.menu-ms #menu>ul>li>ul>li{
	position: relative;
	height: auto;
	/*background: rgba(66,86,104,0);*/
}
.menu-ms #menu>ul>li>ul>li .menuname{
	display: inline-block;
	text-indent: 20px;
	/*text-align: center;*/
}
.menu-ms #menu>ul>li>ul>li>p>.textContainer{
	display: block;
	width: auto;
	margin-left:-10px ;
}
.menu-ms #menu li{
	display: block;
}

