*{
	margin: 0;
	padding: 0;
}
a{
	text-decoration:none;
	color: black;
}

a:hover{
	text-decoration:none;
	color:black;
}
html,body{
	height: 100%;
	width: 100%;
}
ul{
	list-style: none;
}
.container-fluid{
	height: 100%;
	overflow: hidden;
	padding: 0;
}
#menu::-webkit-scrollbar{
	display: none;
}
.container-fluid{
	display: flex;
	width: 100%;
	height: 100%;
}
.container-fluid #leftframe{
	background: #1e4465;
	height: 100%;
}
#rightframe{
	height: 100%;
}
.mainbody{
	display: flex;
	flex: 1;
	flex-direction:column ;
}
#maincontainer{
	display: flex;
	flex-direction: column;
}
.mainbody .changestyle{
	cursor: pointer;
	width: 50px;
	height: 50px;
}

.control_bar{
	display: flex;
	justify-content: space-around;
	align-items: center;
	background: rgba(0,0,0,0.2);
}

.topnav{
	display: flex;
	height: 55px;
	width: 100%;
	background: #EDEDED;
	color: #2A3F54;
}
#menucontainer{
	display: flex;
	flex-direction: column;
	height: 100%;
	color: #D9E4EC;
}
#menucontainer .logo{
	text-align: center;
	height: 50px;
}
#menucontainer .logoName{
	/*text-shadow: 0px 1px 0px #c0c0c0, 0px 2px 0px #b0b0b0, 0px 3px 0px #a0a0a0, 0px 4px 0px #909090, 0px 5px 10px rgba(0, 0, 0, 0.6);*/
	display: inline-block;
	margin-left: 5px;
	font-size: 20px;
	line-height: 20px;
	height: 60px;
	position: relative;
	top: 3px;
}
#menucontainer .logo img{

}
#menucontainer .logo .stagename{
	font-size: 30px;
	position: relative;
	top: 5px;
}
#control_bar{
	width: 100%;
	height: 35px;
	color: white;
	/*bug 360浏览器在底部*/
}
#menu{
	flex: 1;
	height: 100%;
	/*scrollbar-base-color:       rgba(0,0,0,0);
	scrollbar-arrow-color:      rgba(0,0,0,0);
	scrollbar-track-color:      rgba(0,0,0,0);
	scrollbar-3dlight-color:    rgba(0,0,0,0);
	scrollbar-highlight-color:  rgba(0,0,0,0);
	scrollbar-face-color:       rgba(0,0,0,0);
	scrollbar-shadow-color:     rgba(0,0,0,0);
	scrollbar-darkshadow-color: rgba(0,0,0,0);*/
	/*overflow-x:hidden ;*/
}
#menu::-webkit-scrollbar{
	width: 0px;
	height: 8px;
	background-color: rgba(255,255,255,.05);

}

#menu::-webkit-scrollbar-thumb{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
	background-color: #ccc;
	width: 0px;

}
#menu::-webkit-scrollbar-track{
	width: 0px;
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
	border-radius: 10px;
	background-color: rgba(255,255,255,.05);
}
#custommenu{

}
#userinfo{
	width: 135px;
	margin-top: 10px;
}
.topnav #custommenu{
	flex: 1;
}
#userinfo{
	display: block;
}
#maincontainer .tabmenu .centercontainer{
	position: relative;
	height: 30px;
}
#maincontainer .tabmenu .leftarrow{
	position: relative;
	top: 3px;
	height: 27px;
}
#maincontainer .tabmenu .rightarrow{
	position: relative;
	top: 3px;
	height: 27px;

}
#maincontainer .tabmenu .centercontainer ul{
	position: absolute;
	white-space: nowrap;
}
#loaderdiv{
	position: relative;
}

#userinfo .dropdown{
	display: inline-block;
	margin-right:20px;
	height: 50px;
	line-height: 40px;
}
.dropdown #dropdownMenu{
	cursor: pointer;
	user-select:none;
}
#dropdownMenu img{
	width: 30px;
	height: 30px;
	margin-right: 10px;
	border-radius: 50%;
}
#dropdownMenu img span{
	font-size: 10px;
}
#dropdownMenu span{
	-webkit-transform-origin-x: 0;
	-webkit-transform: scale(0.1);
}
#userinfo .dropdown ul{
	position: absolute;
	left:0px;
	top: 40px;
	border: none;
	border-radius:none;
	font-size: 12px;
	padding: 0;

}

.control_bar button{
	background: transparent;
	border: none;
	height: 100%;
	width: 25%;
	color: #5A738E;
}

#menustyle{
	display: inline-block;
	font-size: 25px;
	line-height: 55px;
	margin-left:20px ;
	color: #5A738E;
}
.menu-md{
	width: 230px;
}
.menu-md .logo img{
	margin-top: 10px;
	width: 90%;
}
.menu-md #menu{
	overflow: auto;
	scrollbar:none;
}
.imgContainer{
	width: 90%;
	height: 100%;
	text-align: center;
	margin: 0 auto;
	overflow: hidden;
}
.menu-ms #menu{
	/*overflow: auto;*/
	scrollbar:none;
	position: relative;
}
#menu ul{
	position: relative;
}
.menu-md #menu li img{
	width: 20px;
	position: relative;
	top: -2px;

}
.menu-md #menu li{
	display: block;
	/*必须设置*/
}
.container-fluid .menu-md #menu>ul>li>p{
	/*color: red;*/
	/*background: red;*/
	color: #fff;
	font-size: 14px;
	/*line-height: 24px;*/
}
.menu-md #menu>ul>li>p .textContainer{
	text-indent: 15px;
}
.menu-md #menu>ul>li>p .textContainer .menuname{
	display: inline-block;
	text-indent: 10px;
	line-height: 45px;
	position: relative;
	top: -1px;
	color: #fff;
}
.menu-md #menu>ul>li>p .textContainer .fontspan{
	position: relative;
	left: 0;
	top: 0px;
	font-size: 18px;
}
.menu-md #userinfomation{
	position: relative;
	height: 80px;
}
.menu-md #userinfomation img{
	position: absolute;
	width: 50px;
	height: 50px;
	border-radius: 50%;
	left: 30px;
	top: 10px;
}
.menu-md #userinfomation span{
	position: absolute;
	left: 100px;
	top: 13px;
}
.menu-md #menu p{
	user-select: none;
	display: block;
	height: 45px;
	/*color: white;*/
	line-height: 45px;
	margin: 0;
	font-size: 12px;
}
.menu-md #menu p:hover{
	cursor: pointer;
	color: #fff;
	background: rgba(255,255,255,.05);
}
.menu-md #menu p:active{
	cursor: pointer;
	background: rgba(255,255,255,.1);
}
.menu-md #menu p>span{
	color: #C2CDD8;
	width: 100%;
	display: block;

}
.menu-md #menu p .backspan{
	visibility: hidden;
}
.menu-md #menu p:hover .backspan{
	visibility: hidden;
}
.menu-md #menu p .hassubMenu{
	position: absolute;
	font-size: 12px;
	right: 20px;
}
.menu-md #menu ul>li>ul>li>p{
	height: 35px;
	line-height: 35px;
}
.menu-md #menu ul>li>ul>li>p{

}
.menu-md #menu ul>li>ul>li .fontspan{
	width: 8px;
	height: 8px;
	position: absolute;
	margin: auto;
	top: 0;
	bottom: 0;
	border-radius:50% ;
	left: -5px;
	background: rgba(66,86,104,0.9);
}
.menu-md #menu ul>li>ul>li .textContainer{
	border-left:1px solid rgba(66,86,104,0.9);
	text-indent: 15px;
	width: 80%;
}

.menu-md{
	display: none;
}

@media only screen and (min-width : 800px){
	/*大于992px使用这个样式*/
	/*大于992px的时候大的可以出现,小于就隐藏*/
	.menu-md{
		display: block;
	}
}
.maincontainer .tabmenu{
	background-color: #1e4465;
	display: flex;
	border-bottom: 5px solid rgba(255,255,255,0.4);
}
#maincontainer .tabmenu .centercontainer{
	flex: 1;
	overflow: hidden;
}
#maincontainer .tabmenu .centercontainer ul div{
	border: 1px solid black;
	border-bottom:none;
}
#maincontainer .tabmenu .centercontainer ul div span:nth-child(1){
	text-indent: 10px;
}
#maincontainer .tabmenu .centercontainer ul div .iconfont{
	/*display: none;*/
}
#maincontainer .tabmenu .centercontainer ul div:hover .iconfont{
	display: inline-block;
}
.centercontainer ul{
	margin-bottom: 0px;
	height: 100%;
	transition: all 0.1s linear;
}

.tabmenu>.iconfont{
	color: gray;
	line-height: 30px;
	cursor: pointer;
}
.centercontainer ul div{
	position: relative;
}
.maincontainer .tabmenu ul>div{
	display: inline-block;
	position: relative;
	width: 150px;
	height: 100%;
	margin: 0 1px;
	border-radius: 5px 5px 0px 0px;
	overflow: hidden;
}

.maincontainer .tabmenu ul>div>span{
	display: inline-block;
	height: 100%;
	line-height: 32px;
	cursor: pointer;
	color: white;

}
.maincontainer .tabmenu ul>div>span:nth-child(1){
	width: 100%;
	background-color: rgba(255,255,255,.2);
}
.maincontainer .tabmenu ul>div>span:nth-child(1):hover{
	width: 100%;
	background-color: rgba(255,255,255,.3);
}
.maincontainer .tabmenu ul>div>span:nth-child(2){
	width: 10px;
	height: 10px;
	color: white;
	margin: auto;
	position: absolute;
	top: 0;
	line-height: 10px;
	bottom: 0;
	right: 25px;
	font-size: 12px;
	border-radius: 50%;
}
.maincontainer .tabmenu ul>div>span:nth-child(2):hover{
	color: #00FF00;
}
.maincontainer .tabmenu ul>div>span:nth-child(3){
	width: 10px;
	height: 10px;
	color: white;
	margin: auto;
	position: absolute;
	top: 0;
	line-height: 14px;
	font-size: 14px;
	bottom: 0;
	right: 7px;
	border-radius: 50%;
}
.maincontainer .tabmenu ul>div>span:nth-child(3):hover{
	color: red;
}
#maincontainer{
	height: 100%;
}
#loaderdiv{
	width: 100%;
	flex: 1;
}
#loaderdiv>div{
	position: absolute;
	top: 0;
	left: 0;
	height: 100%;
	width: 100%;
	background-color: white;
}
#loaderdiv iframe{
	width: 100%;
	height: 100%;
	border: none;
}

#lastli:hover p{
	color: #23527C;;
}
.dropdown-menu2 li{
	position: relative;
	width: 130px ;
	height: 40px;
	color: #5A738E;
}
.dropdown-menu2 li>a{
	color: #5A738E;
	display: inline-block;
	position: relative;
	width: 100%;
	height: 40px;
	line-height: 40px;
}
.setright1{
	width: 35px;
	height: 20px;
	background: #E74C3C;
	border: 1px solid #E74C3C!important;
	color: #fff;
	display: inline-block;
	min-width: 10px;
	font-size: 12px;
	font-weight: 700;
