<!--单据详情-->
<div class="cusPopover modal deviceType-modal" id="taskDetailPop"
	 style="display: none;" >
	<div class="modal-container" style="width: 950px; height: 680px; overflow: hidden" >
		<div class="cusPopover-title" style="margin-bottom: 0;">
			<i id="taskDetail-close" class="fa fa-close fa-lg popClose" title="关闭"></i>
			<span>单据详情</span>
		</div>
		<div class="pop-body" style="height: 610px;overflow-y: auto">
			<div class="pop-cont-title">
				基本信息
			</div>
			<div class="pop-cont-box">
				<div class="line-box">
					<div class="lf-text"><i>单据编号：</i></div>
					<div class="rt-text">
						<span id="orderNoSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>单据名称：</i></div>
					<div class="rt-text">
						<span id="taskNameSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>单据描述：</i></div>
					<div class="rt-text">
						<span id="descriptionSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>单据状态：</i></div>
					<div class="rt-text">
						<span id="statusNameSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>创建人员：</i></div>
					<div class="rt-text">
						<span id="createNameSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>工单数量：</i></div>
					<div class="rt-text">
						<span id="orderNumSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>创建时间：</i></div>
					<div class="rt-text">
						<span id="createTimeSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>优先级：</i></div>
					<div class="rt-text">
						<span id="priorityNameSpan"></span>
					</div>
				</div>
			</div>
			<div class="pop-cont-box">
				<div class="line-box">
					<div class="lf-text"><i>维修方案：</i></div>
					<div class="rt-text">
						<span id="planSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>单据开始时间：</i></div>
					<div class="rt-text">
						<span id="startTimeSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>单据完成期限单位：</i></div>
					<div class="rt-text">
						<span id="timeLimitUnitSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>单据完成期限：</i></div>
					<div class="rt-text">
						<span id="timeLimitSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>单据间隔周期单位：</i></div>
					<div class="rt-text">
						<span id="intervalUnitSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>单据间隔周期：</i></div>
					<div class="rt-text">
						<span id="intervalSpan"></span>
					</div>
				</div>
				<div class="line-box">
					<div class="lf-text"><i>工单触发提前时间：</i></div>
					<div class="rt-text">
						<span id="triggerTimeSpan"></span>
					</div>
				</div>
			</div>
			<div class="pop-cont-title">
				设备列表
			</div>
			<div class="wrap-device-pop">
				<table class="table" id="deviceListPop">
					<thead>
					<th style="width: 20%">设备编号</th>
					<th style="width: 20%">商品名称</th>
					<th style="width: 10%">是否自动审核</th>
					<th style="width: 20%">审核人员</th>
					<th style="width: 10%">是否自动派工</th>
					<th style="width: 20%">收货人员</th>
					</thead>
					<tbody>
					</tbody>
				</table>
			</div>

			<div class="hide-num-img" id="hide-device-tips">
				<img src="../../images/common/not-num-tip.png">
				<p>没有设备列表~</p>
			</div>

			<p class="totalNumP">共 <span class="totalDeviceNum"></span> 台关联设备</p>
		</div>
	</div>
</div>

