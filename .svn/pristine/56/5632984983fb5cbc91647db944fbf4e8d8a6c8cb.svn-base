			 ,wms_goods_.goods_code
			 ,wms_goods_.goods_attr
			 ,wms_goods_.goods_type
			 ,wms_goods_.unit
			 ,wms_goods_.has_box_code
			 ,wms_goods_.layer_quantity
			 ,wms_goods_.box_length
			 ,wms_goods_.box_width
			 ,wms_goods_.box_height
			 ,wms_goods_.quality_guarantee_days
			 ,wms_goods_.split_unit
			 ,wms_goods_.split_count
			 ,wms_goods_.gross_weight
			 ,wms_goods_.user_defined1
			 ,wms_goods_.user_defined2
			 ,wms_goods_.user_defined3
			 ,wms_goods_.user_defined4
			 ,wms_goods_.user_defined5
			 ,wms_goods_.create_by
			 ,wms_goods_.gmt_create
			 ,wms_goods_.last_modified_by
			 ,wms_goods_.gmt_modified
			 ,wms_goods_.active_flag
		FROM
			wms_goods  wms_goods_
		where wms_goods_.goods_code in
		<foreach collection="dataList" item="entity" open="(" separator=","
				 close=")">
			#{entity.goodsCode}
		</foreach>
	</select>
