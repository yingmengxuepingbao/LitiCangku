	</select>
	<update id="deleteWmsGoods" parameterType="com.penghaisoft.wms.basicmanagement.model.entity.WmsGoods">
		DELETE FROM wms_goods
		where goods_id = #{entity.goodsId}
	</update>

	<select id="getGoodsCodeAllBatchNo" resultMap="wmsGoodsMap" >
		SELECT
			t.goods_code,
			t.goods_name,
			s.batch_no
		FROM
			wms_goods t
		LEFT JOIN wms_pallet s ON t.goods_code = s.goods_code
		AND s.active_flag = '1'
		WHERE
			t.active_flag = '1'
		GROUP BY
			t.goods_code,
			t.goods_name,
			s.batch_no
	</select>
	<!---根据商品编码分组，排重，获取商品编码，商品名称，批次号-->
	<select id="getHBGoodsCodeAllBatchNo" resultMap="wmsGoodsMap" >
		SELECT
			t.goods_code,
			t.goods_name
		FROM
			wms_goods t
		LEFT JOIN wms_pallet s ON t.goods_code = s.goods_code
		AND s.active_flag = '1'
		WHERE
			t.active_flag = '1'
		GROUP BY
			t.goods_code,
			t.goods_name
	</select>
	<!-- - 根据商品编码，获取批次号-->
	<select id="getHBBatchNo" resultMap="wmsGoodsMap" >
		SELECT
			s.batch_no
		FROM
			wms_pallet s
		LEFT JOIN 	wms_goods t
		ON t.goods_code = s.goods_code
		AND s.active_flag = '1'
		WHERE
			t.active_flag = '1'
		and
			t.goods_code=#{entity.goodsCode}
		GROUP BY
			s.batch_no
	</select>

	<select id="queryBatch" resultMap="wmsGoodsMap">
