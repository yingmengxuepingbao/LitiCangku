		INSERT INTO wms_order_check_pallet_box_barcode (
			order_no,
			diff_flag,
			pallet_code,
			box_barcode,
			remark,
			create_by,
			gmt_create,
			active_flag
		) SELECT
			#{entity.orderNo},
			'1',
			t.pallet_code,
			t.box_barcode,
			#{entity.remark},
			#{entity.createBy},
			#{entity.gmtCreate},
			'1'
		FROM
			wms_box_barcode t
		JOIN wms_pallet s ON t.pallet_code = s.pallet_code
