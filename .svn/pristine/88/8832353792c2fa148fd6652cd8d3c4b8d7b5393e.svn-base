}

.paddset {
	padding: 10px 10px;
	position: relative;
	border-radius: 0 0 5px 5px;
	color: #ccc;
}

.bgset {
	background: url(../../resources/image/wg_bg_02.png) repeat-y center top;
	border-radius: 0 0 5px 5px;
	position: relative;
}

.bgset::before {
	content: " ";
	height: 3px;
	background: url(../../resources/image/wg_bg_01.png) no-repeat center top;
	width: 100%;
	position: absolute;
	top: -3px
}

.bgset::after {
	content: " ";
	height: 2px;
	background: url(../../resources/image/wg_bg_01.png) no-repeat center top;
	width: 100%;
	position: absolute;
	bottom: -2px
}

.common-set>.title {
	height: 30px;
	line-height: 30px;
	width: 100%;
	padding: 0 0 5px 10px;
	background: #858585;
	color: #fff;
	font-size: 14px;
}

.common-set>.title-right-r {
	border-radius: 0 5px 0 0;
}

.common-set>.title-middle-r {
	border-radius: 5px 5px 0 0;
}

.common-set>.title-left-r {
	border-radius: 5px 0 0 0;
}

.common-set>.title-all-r {
	border-radius: 5px 5px 0 0;
}
.bgset>dl {
	padding-top: 5px;
	position: relative;
}

.bgset>dl dt {
	height: 30px;
	line-height: 30px;
	display: block;
	text-align: center;
	color: #333;
}

.bgset>dl dd {
	height: 50px;
	line-height: 40px;
	display: block;
	text-align: center;
	color: #333;
}

.bgset>dl dd i {
	font-size: 34px;
	color: #54a8f6;
	font-style: normal;
	font-weight: 600;
}

.bgset>dl>dt>div {
	position: relative;
	display: block;
	width: 80px;
	margin: 0 auto;
}

.bgset>dl>dt>div img {
	display: inline-block;
	text-align: left;
	position: absolute;
	top: 10px;
	left: 0px;
}

.bgset>dl>dt>div>label {
	position: absolute;
	top: 3px;
	left: 20px;
}

