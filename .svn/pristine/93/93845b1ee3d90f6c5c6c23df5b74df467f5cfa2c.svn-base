<!--				</select>-->
<!--			</div>-->
			<div class="form-group form-inline">
				<input type="button" id="employeeInfoS" class="btn" value="搜索" />
				<input type="button" id="addEmployeeBtn" class="btn"  value="新增单位"/>
			</div>
		</div>
		<!--计量单位-->
		<div class="employee-info">
			<table class="table-hover table" id="employeeListTb"  style="word-break:break-all">
				<thead>
				<tr>
					<th style=" white-space:nowrap;padding-right: 5px">单位代码</th>
					<th style=" white-space:nowrap;padding-right: 5px">单位名称</th>
					<th style=" white-space:nowrap;padding-right: 5px">英文名称</th>
					<th style=" white-space:nowrap;padding-right: 5px">国际度量衡换算值</th>
					<th style=" white-space:nowrap;padding-right: 5px">单位类型</th>
<!--					<th style=" white-space:nowrap;padding-right: 5px">办公电话</th>-->
<!--					<th style=" white-space:nowrap;padding-right: 5px">个人电话</th>-->
<!--					<th style=" white-space:nowrap;padding-right: 5px">邮箱</th>-->
					<th>操作</th></tr>
				</thead>
				<tbody id="personList-table">
				</tbody>
			</table>
			<div class="pagination-con">
				<p class="listNumber rt">
					<button class="btn btn-default btn-xs previousPage"><i class="fa fa-caret-left fa-lg"></i></button>
					<span class="currentPage">1</span>/<span class="pageNumber">1</span>
					<button class="btn btn-default btn-xs nextPage"><i class="fa fa-caret-right fa-lg"></i></button>
					<input class="currentPageBtn" value="1" type="text"  onkeyup="this.value=this.value.replace(/[^0-9]+/g, '');"/>
					<button type="button" id="pageJump"  data-loading-text="请求数据中..." class="btn btn-default pageJump" autocomplete="off">跳转</button>
					每页<span class="pageOneNumber"></span>条，共<span class="dataNumber"></span>条记录
				</p>
			</div>
		</div>
	</div>
</div>
<div id="personPopover" class="cusPopover personw" style="display: none;">
	<div class="modal-container">
		<div class="cusPopover-title">
			<i id="popClose" class="fa fa-close fa-lg popClose" title="关闭"></i>
			<span>单位信息编辑</span>
		</div>
		<div class="pop-body">
			<div class="row-left">
				<div class="form-group form-inline">
					<label for="unitCodePop"><i>*</i>单位代码：</label>
					<input type="text" id="unitCodePop" class="form-control" maxlength="50"  placeholder="允许输入字母、数字、_ -"  onkeyup="this.value=this.value.replace(/[^\w_-]/g,'');"/>
				</div>
				<div class="form-group form-inline">
					<label for="unitZhNamePop"><i>*</i>单位名称：</label>
					<input type="text" id="unitZhNamePop" class="form-control" maxlength="50"  placeholder="请输入单位名称" onkeyup="this.value=this.value.replace(/[=\\=&? ？ *%]/,'')"/>
				</div>
				<div class="form-group form-inline">
					<label for="unitEnNamePop"><i></i>英文名称：</label>
					<input type="text" id="unitEnNamePop" class="form-control" maxlength="50"  placeholder="请输入英文名称" onkeyup="this.value=this.value.replace(/[=\\=&? ？ *%]/,'')"/>
				</div>
				<div class="form-group form-inline">
					<label for="unitChangePop"><i></i>换算值：</label>
					<input type="text" id="unitChangePop" class="form-control" maxlength="50"  placeholder="请输入国际度量衡换算值" onkeyup="this.value=this.value.replace(/[=\\=&? ？ *%]/,'')"/>
				</div>
				<div class="form-group form-inline">
					<label for="unitTypePop">单位类型:</label>
					<select class="form-control" id="unitTypePop">
						<option value=""></option>
						<option value="1">长度</option>
						<option value="2">质量</option>
						<option value="3">温度</option>
						<option value="4">面积</option>
						<option value="5">体积</option>
						<option value="6">压力</option>
						<option value="7">力</option>
						<option value="8">密度</option>
						<option value="9">湿度</option>

					</select>
				</div>
			<i class="fa fa-exclamation-circle fa-personInfoPop" style="position: absolute;top: 400px;left: 30px;"></i>
			<input type="hidden" id="hideUnitIdPop" value="" />
			</div>
			<div class="two-btn">
				<input type="button" id="cancelPersonBtn" class="btn cancel" value="取消"/>
				<input type="button" id="savePersonBtn" class="btn save" value="保存"/>
			</div>
		</div>
	</div>
</div>
